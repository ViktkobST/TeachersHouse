{
  "expo": {
    "name": "Дом учителя",
    "slug": "TeachersHouse",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.viktkobst.TeachersHouse",
      "versionCode": 4,
      "permissions" : [ 
        "CAMERA", 
        "ACCESS_FINE_LOCATION" 
      ]
    }
  }
}
